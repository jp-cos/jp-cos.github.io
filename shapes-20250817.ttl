<https://w3id.org/jp-cos/CommentaryItemShape> a sh:NodeShape;
  sh:targetClass <https://w3id.org/jp-cos/CommentaryItem>;
  sh:property [
    sh:path <http://purl.org/dc/terms/identifier>;
    sh:name "ID"@ja;
    sh:name "ID"@en;
    sh:description "IDは数字7桁（独自のID）。"@ja;
    sh:description "The ID is a 7-digit number. The ID is an original identifier."@en;
    skos:example "1000031";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 1
  ];
  sh:property [
    sh:path <http://schema.org/description>;
    sh:name "テキスト"@ja;
    sh:name "Description"@en;
    sh:description "元データの「学習指導要領解説」の内容。"@ja;
    sh:description "The value of the \"Text of  Commentary on the Course of Study\"."@en;
    skos:example "\"　本内容は，第３学年「Ａ (1) 物と重さ」の学習を踏まえて，「粒子」についての基本的な概念等を柱とした内容のうちの「粒子の保存性」に関わるものであり，第６学年「Ａ (2) 水溶液の性質」の学習につながるものである。\"";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 2
  ];
  sh:property [
    sh:path <http://schema.org/about>;
    sh:name "対象となる学習指導要領細目"@ja;
    sh:name "The target Course of Study item"@en;
    sh:description "本解説で対象としている学習指導要領の細目"@ja;
    sh:description "The Course of Study item addressed in this commentary"@en;
    skos:example <https://w3id.org/jp-cos/8260253110000000>;
    sh:class <https://w3id.org/jp-cos/Item>;
    sh:order 3
  ];
  sh:property [
    sh:path <http://schema.org/citation>;
    sh:name "参照した学習指導要領細目"@ja;
    sh:name "The referenced Course of Study item"@en;
    sh:description "本解説で参照した学習指導要領の細目"@ja;
    sh:description "The Course of Study items referenced in this commentary"@en;
    skos:example <https://w3id.org/jp-cos/8260233110000000>;
    sh:order 4
  ];
  sh:property [
    sh:path <http://schema.org/hasPart>;
    sh:name "子"@ja;
    sh:name "Child"@en;
    sh:description "子供の細目を表す。"@ja;
    sh:description "Child of the item."@en;
    skos:example <https://w3id.org/jp-cos/commentary/1000400>;
    sh:class <https://w3id.org/jp-cos/CommentaryItem>;
    sh:order 5
  ];
  sh:property [
    sh:path <https://w3id.org/jp-cos/commentaryPage>;
    sh:name "学習指導要領解説へのリンク"@ja;
    sh:name "Commentary Page"@en;
    sh:description "対応する学習指導要領解説へのリンク"@ja;
    sh:description "Link to the corresponding Course of Study Commentary with this item."@en;
    skos:example <https://www.mext.go.jp/content/20211020-mxt_kyoiku02-100002607_05.pdf#page=68>;
    sh:maxCount 1;
    sh:nodeKind <http://www.w3.org/ns/shacl#IRI>;
    sh:order 6
  ];
  sh:property [
    sh:path <http://purl.org/dc/terms/title>;
    sh:name "節タイトル"@ja;
    sh:name "Section Title"@en;
    sh:description "細目が節タイトルを表している場合は「テキスト」と同内容を表す。"@ja;
    sh:description "When an item represents a section title, it denotes the same content as the “Description.”"@en;
    skos:example "(1) 物の溶け方";
    sh:maxCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 7
  ];
  sh:property [
    sh:path <http://purl.org/dc/terms/alternative>;
    sh:name "短縮タイトル"@ja;
    sh:name "Abbreviated title"@en;
    sh:description "「節タイトル」の短縮版。パンくずリストなどの利用を想定。"@ja;
    sh:description "Abbreviated section title, intended for use in breadcrumbs and similar contexts."@en;
    skos:example "(1)";
    sh:maxCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#string>;
    sh:order 8
  ];
  sh:property [
    sh:path <http://purl.org/linked-data/cube#order>;
    sh:name "順番"@ja;
    sh:name "Listing order of the item"@en;
    sh:description "表示順を決める場合などで利用"@ja;
    sh:description "This is used in the order of the listings."@en;
    skos:example "1";
    sh:maxCount 1;
    sh:minCount 1;
    sh:datatype <http://www.w3.org/2001/XMLSchema#integer>;
    sh:order 9
  ].

